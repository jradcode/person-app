generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  // Remove the url line completely
}

model Person {
  id       Int    @id @default(autoincrement())
  fullName String
  age      Int?
}

